<html t:type="Border" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<h1>Simple Option Group </h1>

    <t:form t:id="form">
        <select t:type="Select" t:id="entity" t:value="entity" t:model="model"/>

{
    "settings": {
        "number_of_shards": 5
    },
    "mappings": {
        "_default_": {
            "dynamic_templates": [
                {
                    "non_analyzed_string": {
                        "match": "*",
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            ],
            "properties": {
                "created_at": {
                    "type": "date"
                },
                "updated_at": {
                    "type": "date"
                },
                "closed_at": {
                    "type": "date"
                }
                "due_date": {
                  "type":"date"
                },
                "name": {
                    "type": "string",
                    "index": "analyzed"
                },
                "conversions": {
                  "type":"nested"
                }
            }
        },
        "deal": {}
    }
}

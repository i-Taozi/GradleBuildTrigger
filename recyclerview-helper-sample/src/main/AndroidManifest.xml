<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yl.sample">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yl.sample.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoadMoreActivity" />
        <activity android:name=".activity.HeaderAndFooterViewActivity" />
        <activity android:name=".activity.DragAndDropActivity" />
        <activity android:name=".activity.SwipeToDismissActivity" />
        <activity android:name=".activity.SlideItemActivity" />
        <activity android:name=".activity.SuperDividerItemDecorationActivity" />
        <activity android:name=".activity.ClickActivity" />
    </application>

</manifest>